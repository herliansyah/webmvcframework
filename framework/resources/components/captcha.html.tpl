<img src="{CaptchaCode}" id="captcha_code" name="captcha_code">
<br>
<input size="12" type="text" name="captcha_code_verification" id="captcha_code_verification" placeholder="{RES:CapPlaceholder}">
<img border="1" width="20" height="20" src="data:image/png;base64," onclick="reload()">
<script>
    function reload() {
        var xhr = new XMLHttpRequest();
        xhr.open('GET', '{GLOBAL:SITEURL}/{CAPTCHA_RELOAD_SERVICE_ENDPONT}');
        xhr.onload = function() {
            if (xhr.status === 200) {
                // alert(xhr.responseText);
                document.getElementById("captcha_code").src= xhr.responseText;
            }
            else {
                alert('{RES:Captcha_ajax_failure_message}' + xhr.status);
            }
        };
        xhr.send();
    }
</script>
